## getName

method getEnumName param = !{%!GetBaseLarva().GetTypeDefinition()% value!}
method getEnumName returnType = string
          
method getEnumName body =
!{
if (!initDone)
{
    initEnum();
}
return valueToName[value];
!}

## getValue

method getEnumValue param = !{string name!}
method getEnumValue returnType = %!GetBaseLarva().GetTypeDefinition()%
          
method getEnumValue body =
!{
if (!initDone)
{
    initEnum();
}
return nameToValue[name];
!}

## initEnum

method initEnum param = !{!}
method initEnum returnType = void
       

method initEnum body enum = 
!{
valueToName[%value%] = "%description%";
nameToValue["%description%"] = %value%;
!}

method initEnum body =
!{
    valueToName = new %!Parts[1].Larva.GetTypeDefinition()%();
    nameToValue = new %!Parts[2].Larva.GetTypeDefinition()%();
    %%
    initDone = true;
!}